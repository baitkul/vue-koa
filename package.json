{
  "name": "vue-koa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "PORT=3030 HOST=localhost vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node -r dotenv/config -r esm ./server",
    "dev": "nodemon -r dotenv/config -r esm ./server",
    "knex": "./node_modules/.bin/knex",
    "db:create": "echo 'CREATE DATABASE `koa` CHARACTER SET `utf8` COLLATE `utf8_general_ci`' | mysql -u root -p"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "element-ui": "^2.13.0",
    "esm": "^3.2.25",
    "faker": "^4.1.0",
    "knex": "^0.20.8",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-joi-router": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "moment": "^2.24.0",
    "mysql": "^2.18.0",
    "qs": "^6.9.1",
    "tachyons": "^4.11.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
