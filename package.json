{
  "name": "vue-koa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "PORT=3030 HOST=localhost vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node -r dotenv/config -r esm ./server",
    "dev": "nodemon -r dotenv/config -r esm ./server",
    "knex": "./node_modules/.bin/knex",
    "db:create": "echo 'CREATE DATABASE `koa` CHARACTER SET `utf8` COLLATE `utf8_general_ci`' | mysql -u root -p"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "element-ui": "^2.3.8",
    "esm": "^3.0.34",
    "faker": "^4.1.0",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-joi-router": "^5.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-static": "^4.0.2",
    "moment": "^2.22.1",
    "mysql": "^2.15.0",
    "qs": "^6.5.2",
    "tachyons": "^4.9.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.10",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.4",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
